<% if (theme.background.enable && theme.background.images && theme.background.length > 0) { 
    var randomImage = theme.background.images + Math.floor(Math.random() * theme.background.images.length) + ".jpg";
%>
    <style>
        body{
            background-image: url('<%- randomImage %>');
            background-repeat: no-repeat;
            background-size: 100% 100%;
            background-attachment: fixed;
        }
    </style>
<% } %>